{
 "globals": {
  "EMOJIS": true,
  "PRODUCTION": true,
  "SCOPE_VERSION": true,
  "TRANSLATIONS": true,
  "oc_userconfig": true,
  "appName": true,
  "appVersion": true
 },
 "extends": [
  "@nextcloud/eslint-config/vue3",
  "plugin:storybook/recommended"
 ],
 "parserOptions": {
  "babelOptions": {
   "plugins": [
    "@babel/plugin-syntax-import-assertions"
   ]
  }
 },
 "rules": {
  "@nextcloud/no-deprecations": [
   "error",
   {
    "parseAppInfo": false
   }
  ],
  "@nextcloud/no-removed-apis": [
   "error",
   {
    "parseAppInfo": false
   }
  ],
  "import/no-unresolved": [
   "error",
   {
    "ignore": [
     "\\?raw$"
    ]
   }
  ]
 }
}
