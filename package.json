    "@vue/test-utils": "^1.3.0",
    "@vue/tsconfig": "^0.4.0",
    "@vue/vue2-jest": "^29.0.0",
    "babel-jest": "^29.0.1",
    "babel-loader-exclude-node-modules-except": "^1.2.1",
    "cypress": "^13.1.0",
    "cypress-visual-regression": "^4.0.0",
    "eslint-plugin-cypress": "^2.11.1",
