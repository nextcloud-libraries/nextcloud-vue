{
  "name": "@nextcloud/vue",
  "version": "9.0.0-alpha.6",
  "description": "Nextcloud vue components",
  "keywords": [
    "vuejs",
    "nextcloud",
    "components"
  ],
  "homepage": "https://github.com/nextcloud-libraries/nextcloud-vue",
  "bugs": "https://github.com/nextcloud-libraries/nextcloud-vue/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nextcloud-libraries/nextcloud-vue.git"
  },
  "license": "AGPL-3.0-or-later",
  "author": "Nextcloud GmbH and Nextcloud contributors",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    },
    "./components/*": {
      "types": "./dist/components/*/index.d.ts",
      "import": "./dist/components/*/index.mjs"
    },
    "./composables/*": {
      "types": "./dist/composables/*/index.d.ts",
      "import": "./dist/composables/*/index.mjs"
    },
    "./directives/*": {
      "types": "./dist/directives/*/index.d.ts",
      "import": "./dist/directives/*/index.mjs"
    },
    "./functions/*": {
      "types": "./dist/functions/*/index.d.ts",
      "import": "./dist/functions/*/index.mjs"
    },
    "./mixins/*": {
      "types": "./dist/mixins/*/index.d.ts",
      "import": "./dist/mixins/*/index.mjs"
    }
  },
  "files": [
    "CHANGELOG.md",
    "LICENSE",
    "README.md",
    "dist"
  ],
  "scripts": {
    "build": "vite build --mode production ",
    "dev": "vite build --mode development",
    "dev:watch": "vite build --mode development --watch",
    "l10n:extract": "node build/extract-l10n.mjs",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "styleguide": "vue-styleguidist --config styleguide.config.cjs server",
    "styleguide:build": "vue-styleguidist --config styleguide.config.cjs build",
    "stylelint": "stylelint \"src/**/*.vue\" \"src/**/*.scss\" \"src/**/*.css\"",
    "stylelint:fix": "stylelint \"src/**/*.vue\" \"src/**/*.scss\" \"src/**/*.css\" --fix",
    "test": "TZ=UTC vitest run",
    "test:component": "playwright test -c playwright.config.ts",
    "test:component:gui": "playwright test --ui -c playwright.config.ts",
    "test:coverage": "TZ=UTC vitest run --coverage",
    "update:snapshots": "npm run test:component -- --grep @visual --update-snapshots",
    "watch": "npm run dev:watch"
  },
  "browserslist": [
    "extends @nextcloud/browserslist-config"
  ],
  "dependencies": {
    "@ckpack/vue-color": "^1.6.0",
    "@floating-ui/dom": "^1.6.13",
    "@nextcloud/auth": "^2.4.0",
    "@nextcloud/axios": "^2.5.1",
    "@nextcloud/browser-storage": "^0.4.0",
    "@nextcloud/capabilities": "^1.2.0",
    "@nextcloud/event-bus": "^3.3.1",
    "@nextcloud/initial-state": "^2.2.0",
    "@nextcloud/l10n": "^3.1.0",
    "@nextcloud/logger": "^3.0.2",
    "@nextcloud/router": "^3.0.1",
    "@nextcloud/sharing": "^0.2.4",
    "@nextcloud/timezones": "^0.1.1",
    "@vueuse/components": "^12.5.0",
    "@vueuse/core": "^12.4.0",
    "clone": "^2.1.2",
    "debounce": "^2.2.0",
    "dompurify": "^3.2.4",
    "emoji-mart-vue-fast": "^15.0.3",
    "escape-html": "^1.0.3",
    "floating-vue": "^5.2.2",
    "focus-trap": "^7.6.4",
    "linkify-string": "^4.2.0",
    "md5": "^2.3.0",
    "rehype-external-links": "^3.0.0",
    "rehype-highlight": "^7.0.2",
    "rehype-react": "^8.0.0",
    "remark-breaks": "^4.0.0",
    "remark-gfm": "^4.0.0",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.1",
    "splitpanes": "^3.1.8",
    "string-length": "^6.0.0",
    "striptags": "^3.2.0",
    "tributejs": "^5.1.3",
    "unified": "^11.0.5",
    "unist-builder": "^4.0.0",
    "unist-util-visit": "^5.0.0",
    "vue": "^3.5.13",
    "vue-datepicker-next": "^1.0.3",
    "vue-router": "^4.5.0",
    "vue-select": "^4.0.0-beta.6"
  },
  "devDependencies": {
    "@babel/plugin-syntax-import-assertions": "^7.26.0",
    "@babel/plugin-transform-typescript": "^7.26.7",
    "@babel/preset-typescript": "^7.26.0",
    "@fontsource/roboto": "^5.1.1",
    "@mdi/js": "^7.4.47",
    "@mdi/svg": "^7.4.47",
    "@nextcloud/babel-config": "^1.2.0",
    "@nextcloud/browserslist-config": "^3.0.1",
    "@nextcloud/eslint-config": "^8.4.1",
    "@nextcloud/stylelint-config": "^3.0.1",
    "@nextcloud/vite-config": "^2.3.1",
    "@nextcloud/webpack-vue-config": "github:nextcloud/webpack-vue-config#vue3",
    "@playwright/experimental-ct-vue": "^1.50.1",
    "@playwright/test": "^1.50.1",
    "@types/gettext-parser": "^4.0.4",
    "@types/node": "^22.13.1",
    "@vitest/coverage-v8": "^3.0.3",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "babel-loader-exclude-node-modules-except": "^1.2.1",
    "core-js": "^3.40.0",
    "file-loader": "^6.2.0",
    "gettext-extractor": "^3.8.0",
    "gettext-parser": "^8.0.0",
    "glob": "^11.0.1",
    "jsdom": "^26.0.0",
    "resolve-url-loader": "^5.0.0",
    "sanitize-filename": "^1.6.3",
    "sass": "^1.83.4",
    "stylelint": "^16.14.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "url-loader": "^4.1.1",
    "vite": "^6.0.11",
    "vitest": "^3.0.5",
    "vue-eslint-parser": "^9.4.3",
    "vue-material-design-icons": "^5.3.1",
    "vue-styleguidist": "^4.72.4",
    "webpack": "^5.97.1",
    "webpack-merge": "^6.0.1"
  },
  "engines": {
    "node": "^20.11.0",
    "npm": "^10.0.0"
  }
}
